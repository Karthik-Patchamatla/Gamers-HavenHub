{
    "version": 2,
    "projects": [
      {
        "src": "Backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/Frontend/build/index.html"
      }
    ]
  }
  